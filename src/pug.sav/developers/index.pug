include ../includes/common.puginclude ../includes/data.pug-    var currPage = $i18n.PAGES[2];    htmlAttrs = currPage.htmlAttrs;include ../includes/headbody(class=$i18n.lang)    include ../includes/header    app-developers        section            .screen-main                .container                    img.main(src="/assets/images/dev-main.svg")                    img.main-m(src="/assets/images/dev-main-m.svg")                    each item, idx in $i18n.DEVELOPERS.MAP                        +developers-map-item( item, idx )                .dummy                a.arrow-down.page-scroll(href="#learnabout")                    img(src="/assets/icons/dev-arrow-down.svg")            .screen-learn-about#learnabout(data-offset=50)                .container                    h2!=$i18n.DEVELOPERS.LEARN_ABOUT.h2                    section                        each item in $i18n.DEVELOPERS.LEARN_ABOUT.LIST                            +developers-learn-about-item(item)            .screen-get-started                .container                    h2!=$i18n.DEVELOPERS.GET_STARTED.h2                    section                        article                            h4!=$i18n.DEVELOPERS.GET_STARTED.QUICKSTART.h3                            .text!=$i18n.DEVELOPERS.GET_STARTED.QUICKSTART.text                            .video-section                                .video-wrapper                                    video-js#singleVideo(preload='no' crossorigin='' controls=true poster!=$i18n.DEVELOPERS.GET_STARTED.QUICKSTART.video.poster data-setup='{"fluid": true }')                                        source(src!=$i18n.DEVELOPERS.GET_STARTED.QUICKSTART.video.src)                        .sep                            h4 OR                        article                            h4!=$i18n.DEVELOPERS.GET_STARTED.TESTNET.h3                            .text!=$i18n.DEVELOPERS.GET_STARTED.TESTNET.text                            a.link(href!=$i18n.DEVELOPERS.GET_STARTED.TESTNET.link.href target!=$i18n.DEVELOPERS.GET_STARTED.TESTNET.link.target)!=$i18n.DEVELOPERS.GET_STARTED.TESTNET.link.title            .screen-meet-molis                .container                    h2!=$i18n.DEVELOPERS.MOLIS.h2                    section                        .text!=$i18n.DEVELOPERS.MOLIS.text                        //a.link(href!=$i18n.DEVELOPERS.MOLIS.link.href target!=$i18n.DEVELOPERS.MOLIS.link.target)!=$i18n.DEVELOPERS.MOLIS.link.title                        h4!=$i18n.DEVELOPERS.MOLIS.DOWNLOAD.h4                        .list                            each item in $i18n.DEVELOPERS.MOLIS.DOWNLOAD.LIST                                +developers-molis-download-item(item)            .screen-apla-github                .container                    h2!=$i18n.DEVELOPERS.GITHUB.h2                    section                        .left                            img(src="/assets/icons/source_code.png")                            .wrap                                | Check out our&nbsp;                                a.link(href!=$i18n.DEVELOPERS.GITHUB.backend.href target!=$i18n.DEVELOPERS.GITHUB.backend.target)!=$i18n.DEVELOPERS.GITHUB.backend.title                                | &nbsp;or &nbsp;                                a.link(href!=$i18n.DEVELOPERS.GITHUB.frontend.href target!=$i18n.DEVELOPERS.GITHUB.frontend.target)!=$i18n.DEVELOPERS.GITHUB.frontend.title                                | &nbsp;repositories                        .right                            img.s(src="/assets/icons/contribute.svg")                            .wrap                                a.link(href!=$i18n.DEVELOPERS.GITHUB.contib.href target!=$i18n.DEVELOPERS.GITHUB.contib.target)!=$i18n.DEVELOPERS.GITHUB.contib.title                                | &nbsp;to development                            img.b(src="/assets/icons/contribute.svg")            .screen-join                .container                    .w                        h2!=$i18n.DEVELOPERS.JOIN.h2                        a.link1(href!=$i18n.DEVELOPERS.JOIN.link_1.href target!=$i18n.DEVELOPERS.JOIN.link_1.target)                            img(src="/assets/icons/telegram_white.svg")                            .wrap!=$i18n.DEVELOPERS.JOIN.link_1.title                    .text!=$i18n.DEVELOPERS.JOIN.text                    a.link.link-2(href!=$i18n.DEVELOPERS.JOIN.link_2.href target!=$i18n.DEVELOPERS.JOIN.link_2.target)!=$i18n.DEVELOPERS.JOIN.link_2.title        //include ../includes/banner        //include ../includes/newsletter        include ../includes/footer        include ../includes/popupform