include /pug/includes/common.puginclude /pug/includes/data.pug-    var currPage = $i18n.PAGES[2];    htmlAttrs = currPage.htmlAttrs;include /pug/includes/headbody(class=$i18n.lang)    include /pug/includes/header    app-developers        section            .screen-main                .container                    img.main(src="/assets/images/dev-main-2.svg")                    img.main-m(src="/assets/images/dev-main-m-2.svg")                    each item, idx in $i18n.DEVELOPERS.MAP                        +developers-map-item( item, idx )                .dummy                a.arrow-down.page-scroll(href="#learnabout")                    img(src="/assets/icons/dev-arrow-down.svg")            .screen-learn-about#learnabout(data-offset=50)                .container                    h2!=$i18n.DEVELOPERS.LEARN_ABOUT.h2                    section                        each item in $i18n.DEVELOPERS.LEARN_ABOUT.LIST                            +developers-learn-about-item(item)            .screen-apla-github                .container                    h2!=$i18n.DEVELOPERS.GITHUB.h2                    section                        .left                            img(src="/assets/icons/source_code.png")                            .wrap                                | Check out our&nbsp;                                a.link(href!=$i18n.DEVELOPERS.GITHUB.backend.href target!=$i18n.DEVELOPERS.GITHUB.backend.target)!=$i18n.DEVELOPERS.GITHUB.backend.title                                | &nbsp;or &nbsp;                                a.link(href!=$i18n.DEVELOPERS.GITHUB.frontend.href target!=$i18n.DEVELOPERS.GITHUB.frontend.target)!=$i18n.DEVELOPERS.GITHUB.frontend.title                                | &nbsp;repositories                        .right                            img.s(src="/assets/icons/contribute.svg")                            .wrap                                a.link(href!=$i18n.DEVELOPERS.GITHUB.contib.href target!=$i18n.DEVELOPERS.GITHUB.contib.target)!=$i18n.DEVELOPERS.GITHUB.contib.title                                | &nbsp;to development                            img.b(src="/assets/icons/contribute.svg")        //include /pug/includes/banner        //include /pug/includes/newsletter        include /pug/includes/footer        include /pug/includes/popupform